# Configurações do Frontend DL Sistema
# Copie este arquivo para .env.local e ajuste conforme necessário

# URL base da API do backend (obrigatório)
NEXT_PUBLIC_API_URL=http://localhost:8001/api/v1

# Ambiente da aplicação
NEXT_PUBLIC_ENV=development

# Nome da aplicação
NEXT_PUBLIC_APP_NAME=DL Auto Peças

# Versão da aplicação
NEXT_PUBLIC_APP_VERSION=1.0.0

# Configurações de debug (apenas desenvolvimento)
NEXT_PUBLIC_DEBUG=true 